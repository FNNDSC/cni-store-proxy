{
  "name": "cni-store-proxy",
  "version": "1.0.0",
  "description": "Reverse proxy for the ChRIS_store API to automatically register and run uploaded plugins",
  "main": "server.js",
  "scripts": {
    "serve": "node server.js",
    "postinstall": "git submodule update --init && ln -sv test/local.env .env",
    "prestart": "yarn run cube:restart && ./prepare.sh",
    "start": "yarn run serve",
    "cube:up": "test/cube/minimake.sh",
    "cube:down": "test/cube/unmake.sh",
    "cube:restart": "yarn run cube:down && yarn run cube:up",
    "pretest": "yarn run prestart",
    "test": "test/e2e.sh",
    "posttest": "yarn run cube:down"
  },
  "repository": "https://github.com/fnndsc/cni-store-proxy",
  "author": "Jennings Zhang <Jennings.Zhang@childrens.harvard.edu>",
  "license": "MIT",
  "private": false,
  "dependencies": {
    "axios": "^0.19.2",
    "colors": "^1.4.0",
    "express": "^4.17.1",
    "http-proxy": "^1.18.1"
  },
  "devDependencies": {
    "dotenv": "^8.2.0"
  }
}
